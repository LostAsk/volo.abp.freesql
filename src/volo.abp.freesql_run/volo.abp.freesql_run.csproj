        <TargetFramework>net5.0</TargetFramework>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="FreeSql.Provider.SqlServer" Version="2.3.106" />
        <PackageReference Include="Volo.Abp.Autofac" Version="4.2.1" />
